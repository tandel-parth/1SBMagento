<config>
    <tabs>
        <jethalal translate="label" module="jethalal">
            <label>JETHALAL</label>
            <sort_order>100</sort_order>
        </jethalal>
    </tabs>
    <sections>
        <jethalal translate="label" module="jethalal">
            <label>Jethalal</label>
            <tab>jethalal</tab>
            <frontend_type>text</frontend_type>
            <sort_order>100</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <general translate="label">
                    <label>General Settings</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>10</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <jethalal_enabled translate="label">
                            <label>Enable Jethalal Module</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </jethalal_enabled>
                        <jethalal_checkbox translate="label">
                            <label>Jalebi Title</label>
                            <frontend_type>multiselect</frontend_type>
                            <source_model>ccc_jethalal/jalebi</source_model>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </jethalal_checkbox>
                        <jethalal_radio translate="label">
                            <label>Jalebi Title</label>
                            <frontend_type>radios</frontend_type>
                            <source_model>ccc_jethalal/jalebi</source_model>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </jethalal_radio>
                        <jethalal_text translate="label">
                            <label>Jethalal Text</label>
                            <frontend_type>textarea</frontend_type>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </jethalal_text>
                        <jethalal_title translate="label">
                            <label>Jethalal Title</label>
                        </jethalal_title>
                    </fields>
                </general>
            </groups>
        </jethalal>
    </sections>
</config>